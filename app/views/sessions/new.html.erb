<div id="container">
  <h2>Administrator Login</h2>
<p>
  <% if flash[:error] -%>
  <p class='error'><%=h flash[:error] %></p>
  <% end -%>
</p>  

 <%= form_for :session, url: login_path do |f| %>
  <p>
  <%= f.label :email %><br>
  <%= f.email_field :email, placeholder: "email" %>
  </p>

  <p>
  <%= f.label :password %><br>
  <%= f.password_field :password, placeholder: "password" %>  
  </p>

  <p><%= f.submit "Log in"%></p> 
<% end %> 
</div>