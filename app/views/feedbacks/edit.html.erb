<%= render 'welcome/navbar' %> <!-- Navigation Bar -->

<h1>Feedback Approval Form</h1>

<%= form_for :feedback, url: feedback_path, method: :patch do |f| %>
  <p><%= f.label :commenter %>: <%= f.text_field :commenter %></p>
  <p><%= f.label :is_approved %>: <%= f.check_box :is_approved %></p>
  <p><%= f.label :body %><br><%= f.text_area :body %></p>
  <p><%= f.submit %></p>
<% end %>

<br><br><br><br><br> <!-- Much needed whitespace -->

<%= link_to 'Back', feedbacks_path %>